load("davis-putnam.stlx");

m := parse("{ {p, q}, {p, !q}, {!p, q}, {!p, !q} }");
r := davisPutnam( m, {} );
print(" r = ", r);

m := parse("{ {p, q, s}, {!p, r, !v}, {r, s}, {!r, q, !p}, " + 
           "{!s, p}, {!p, !q, s, !r}, {p, !q, s}, {!r, !s}, {!p, !s} }");
r := davisPutnam( m, {} );
print(" r = ", r);

m := parse("{ {p, q, s}, {!p, r, !t}, {r, s, p}, {!r, q, !p}, {!s, p}, {!p, !q, s, !r}, {p, !q, s}, {!r, !s}, {!p, !s} }");
r := davisPutnam( m, {} );
print(" r = ", r);

m := parse("{ {p, q, s}, {!p, r, !t}, {r, s}, {!r, q, !p}, {!s, p}, {!p, !q, s, !r}, {p, !q, s}, {!r, !s}, {!p, !s} }");
r := davisPutnam( m, {} );
print(" r = ", r);
