// Die Prozedur vielfache(m, n) berechnet alle Vielfachen von m bis zum
// n-ten Vielfachen.
vielfache := [m, n] |-> { m * i : i in { 1 .. n } };

// Die Prozedur kgv(m, n) berechnet das kleinste gemeinsame Vielfache
// von m und n.
kgv := procedure(m, n) {
  return min(vielfache(m, n) * vielfache(n, m));
};

print( kgv( 18, 24 ) );
